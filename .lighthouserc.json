{
    "ci": {
        "collect": {
            "startServerCommand": "npm run dev",
            "url": [
                "http://localhost:4321/"
            ],
            "numberOfRuns": 1
        },
        "assert": {
            "assertions": {
                "categories:performance": [
                    "error",
                    {
                        "minScore": 0.95
                    }
                ],
                "categories:accessibility": [
                    "error",
                    {
                        "minScore": 1
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 1
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}