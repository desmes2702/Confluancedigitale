{
  "layouts": {
    "BaseLayout.astro": {
      "path": "src/layouts/BaseLayout.astro",
      "purpose": "Main layout with Header and Footer",
      "props": [
        "title: string",
        "description?: string",
        "headerTransparent?: boolean"
      ],
      "children": [
        "Header",
        "slot",
        "Footer"
      ],
      "imports": [
        "import Header from '@/components/Header.astro'",
        "import Footer from '@/components/Footer.astro'"
      ],
      "example": "---\nimport Header from '@/components/Header.astro';\nimport Footer from '@/components/Footer.astro';\n\ninterface Props {\n  title: string;\n  description?: string;\n  headerTransparent?: boolean;\n}\n\nconst { title, description, headerTransparent = false } = Astro.props;\n---\n<!DOCTYPE html>\n<html lang=\"fr\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <meta name=\"viewport\" content=\"width=device-width\" />\n  <title>{title} - Confluence Digitale</title>\n  {description && <meta name=\"description\" content={description} />}\n</head>\n<body>\n  <Header transparent={headerTransparent} />\n  <slot />\n  <Footer />\n</body>\n</html>"
    }
  },
